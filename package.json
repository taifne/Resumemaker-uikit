{
  "name": "taine-component",
  "version": "0.1.7",
  "private": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "app/**/*",
    "component/**/*"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "rimraf dist && tsc",
    "build:prod": "rimraf dist && tsc -p ./tsconfig.build.json && tscpaths -p ./tsconfig.build.json -s ./src -o ./dist  ",
    "start": "next start",
    "lint": "next lint",
    "rollup": "rollup -c --bundleConfigAsCjs && sh ./scripts/copy_components.sh"
  },
  "dependencies": {
    "next": "14.2.16",
    "npm": "^10.9.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.16",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tscpaths": "^0.0.9",
    "tslib": "^2.6.2",
    "typescript": "^5"
  }
}
